//1st programme
print("Enter the radius of circle is:")
radius=float(input())
area=22/7*(radius**2)
print("The area of thr circle with radius 1.1 is ",area)

//2nd programme
print("input the file name")
filename=input()
if filename=="abc.py":
  print("The extension of file is :'python'")
elif filename=="abc.cpp":
    print("The extension of file is:'c++'")
elif filename=="abc.html":
    print("The extension of file is:'HTML'")
else :
  print ("please input valid file name")
